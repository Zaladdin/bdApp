{
  "name": "birthday-organizer",
  "version": "1.0.0",
  "description": "Веб-приложение для организации дня рождения",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": ["birthday", "party", "organizer", "wishlist"],
  "author": "Your Name",
  "license": "MIT"
}

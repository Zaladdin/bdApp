{
  "name": "birthday-organizer",
  "version": "1.0.0",
  "description": "Веб-приложение для организации дня рождения",
  "main": "server/index.js",
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "lucide-react": "^0.294.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.6",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "cd server && nodemon index.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "vercel-build": "npm run build",
    "check-deploy": "node check-deployment.js",
    "deploy": "vercel --prod",
    "deploy-public": "vercel --prod --public --name birthday-organizer",
    "deploy-win": "deploy.bat",
    "deploy-linux": "chmod +x deploy.sh && ./deploy.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "birthday",
    "party",
    "organizer",
    "web",
    "react"
  ],
  "author": "Zaladdin",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
